{
  "name": "predictive-healthcare",
  "version": "1.0.0",
  "description": "A predictive healthcare application for disease prediction and patient management",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm src/scripts/start-dev.ts",
    "build": "next build && npm run optimize",
    "start": "next start",
    "lint": "next lint",
    "clean": "node --loader ts-node/esm src/scripts/clean.ts",
    "predev": "npm run clean",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "optimize": "node --loader ts-node/esm src/scripts/optimize.ts",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.17.0",
    "@types/pg": "^8.11.14",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.3.2",
    "mongoose": "^8.1.3",
    "next": "^14.1.0",
    "pg": "^8.15.6",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.55",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
